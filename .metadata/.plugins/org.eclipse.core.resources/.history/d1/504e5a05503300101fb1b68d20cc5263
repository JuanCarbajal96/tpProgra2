package ar.edu.ungs.prog2.ticketek;

public class Sector {

	private String nombre;
	private int capacidad;
	private	int porcentajeAdicional;
	private double valorFijoConsumicion;

	public Sector( String nombre, int capacidad, int porcentajeAdicional) {

		this.nombre = nombre;
		this.capacidad = capacidad;
		this.porcentajeAdicional = porcentajeAdicional;	

	}

	public Sector(String nombre, int capacidad, int porcentajeAdicional, double valorFijoConsumicion) {
		this.nombre = nombre;
		this.capacidad = capacidad;
		this.porcentajeAdicional = porcentajeAdicional;
		this.valorFijoConsumicion = valorFijoConsumicion;
	}



}
