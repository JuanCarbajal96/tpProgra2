package ar.edu.ungs.prog2.ticketek;

import java.util.ArrayList;

public class Sector {

	String nombre;
	int capacidad;
	int porcentajeAdicional;

	public Sector( String nombre, int capacidad, int porcentajeAdicional) {

		this.nombre = nombre;
		this.capacidad = capacidad;
		this.porcentajeAdicional = porcentajeAdicional;		

	}

	private ArrayList<Sector> agregarSectores(String[] sectores2, int[] capacidad, int[] porcentajeAdicional) {

		for (int i = 0; i < sectores2.length; i++) {

			Sector sector = new Sector(sectores2[i],capacidad[i],porcentajeAdicional[i]);

		}
	}
}
