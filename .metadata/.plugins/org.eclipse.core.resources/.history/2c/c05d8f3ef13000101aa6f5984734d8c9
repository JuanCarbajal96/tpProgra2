package ar.edu.ungs.prog2.ticketek;

import java.util.ArrayList;

public class Teatro extends Sede {
	
	ArrayList<Sector> sectores;

	public Teatro(String nombre, String direccion, int capacidadMaxima, int asientosPorFila,
			String[] sectores, int[] capacidad, int[] porcentajeAdicional) {
		super(nombre, direccion, capacidadMaxima);
		
		int cantidadSectores = sectores.length;
		
		for (int i = 0; i < cantidadSectores; i++) {
			
			Sector sector = new Sector(sectores[i],capadidad[i],porcentajeAdicional[i]);
			
		} {
			
			
		}
		
		
		this.sectores = new ArrayList<Sector>();
		
//		Sector sector = new Sector (asientosPorFila,sectores,capacidad,porcentajeAdicional);
		
		// ver si corresponde poner exepciones.
		
		this.sectores.add(sector);
		
		
		
		
		
		
		
		
		
	}
	
	
	

}
