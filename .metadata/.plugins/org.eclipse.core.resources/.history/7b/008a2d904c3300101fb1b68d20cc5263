package ar.edu.ungs.prog2.ticketek;

public class Sector {

	String nombre;
	int capacidad;
	int porcentajeAdicional;
	int valorFijoConsumicion;
	
	public Sector( String nombre, int capacidad, int porcentajeAdicional) {
		
		this.nombre = nombre;
		this.capacidad = capacidad;
		this.porcentajeAdicional = porcentajeAdicional;	
		
	}

	public Sector(String nombre, int capacidad, int porcentajeAdicional, int valorFijoConsumicion) {
		this.nombre = nombre;
		this.capacidad = capacidad;
		this.porcentajeAdicional = porcentajeAdicional;
		this.valorFijoConsumicion = valorFijoConsumicion;
	}
	
	

}
