package ar.edu.ungs.prog2.ticketek;

public class Sector {
	
	int asientosPorFilas;
	String nombre;
	int capacidad;
	int porcentajeAdicional;
	
	public Sector(int asientosPorFilas, String nombre, int capacidad, int porcentajeAdicional) {
		
		this.asientosPorFilas = asientosPorFilas;
		this.nombre = nombre;
		this.capacidad = capacidad;
		this.porcentajeAdicional = porcentajeAdicional;		
		
	}

}
